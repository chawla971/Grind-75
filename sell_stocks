class Solution:
    def maxProfit(self, prices: list[int]) -> int:
        l,r=0,1
        maxP=0
        
        while r<len(prices):
            if prices[l]<prices[r]:
                profit=prices[r]-prices[l]
                maxP=max(maxP,profit)
            else:
                l=r
            r+=1
        return maxP

if __name__ == "__main__":
    solver = Solution()
    print(solver.maxProfit([7,1,5,3,6,4]))  # Output: 5
    print(solver.maxProfit([7,6,4,3,1]))    # Output: 0
    